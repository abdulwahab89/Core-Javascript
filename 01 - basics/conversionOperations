// POSTFIX AND PREFIX

// postfix operator returns a value and then increments it.
//example [postfix]

let y=4;
let z=y++
console.log("Postfix Icrement of z:"+ z)
console.log(y);

// prefix operator increments a value and then returns it.
//example [prefix]

let t=4;
let j=++y
console.log("PreFix icrement of : "+j)
console.log(t);
